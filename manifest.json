{
  "manifest_version": 2,

  "name": "Custom New Tab - IDD",
  "description": "This extension allows you to have a custom new tab.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon16.png"
  },
  "chrome_url_overrides" : {
    "newtab" : "newtab.html"
  },
  "icons" : {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "148" : "icon128.png"
  }
}
